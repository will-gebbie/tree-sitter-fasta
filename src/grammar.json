{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "fasta",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "record_definition_description"
        },
        {
          "type": "SYMBOL",
          "name": "record_definition_no_description"
        }
      ]
    },
    "record_definition_description": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "id"
        },
        {
          "type": "SYMBOL",
          "name": "space"
        },
        {
          "type": "SYMBOL",
          "name": "description"
        },
        {
          "type": "SYMBOL",
          "name": "sequence"
        }
      ]
    },
    "record_definition_no_description": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "id"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        },
        {
          "type": "SYMBOL",
          "name": "sequence"
        }
      ]
    },
    "id": {
      "type": "SYMBOL",
      "name": "id_text"
    },
    "description": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "description_text"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "sequence": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "sequence_choice"
      }
    },
    "sequence_choice": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sequence_newline"
        },
        {
          "type": "SYMBOL",
          "name": "sequence_EOF"
        }
      ]
    },
    "sequence_newline": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sequence_text"
        },
        {
          "type": "SYMBOL",
          "name": "newline"
        }
      ]
    },
    "sequence_EOF": {
      "type": "SYMBOL",
      "name": "sequence_text"
    },
    "id_text": {
      "type": "PATTERN",
      "value": ">[!-=?-~]+"
    },
    "description_text": {
      "type": "PATTERN",
      "value": "[ -~]+"
    },
    "newline": {
      "type": "PATTERN",
      "value": "[\\r?\\n]"
    },
    "space": {
      "type": "PATTERN",
      "value": " +"
    },
    "sequence_text": {
      "type": "PATTERN",
      "value": "[a-zA-Z*-]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}